            <th>
                @Html.DisplayNameFor(model => model.RepName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RepEmail)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CreatedTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CreatedByNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
